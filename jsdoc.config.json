{
    "plugins": [
        "node_modules/jsdoc-vuejs"
    ],
    "recurseDepth": 10,
    "source": {
        "include": [
            "src/components/calendar",
            "src/components/pinia"
        ],
        "includePattern": ".+\\.(js|vue)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs"
    }
}